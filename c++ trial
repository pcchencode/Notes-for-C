#include <iostream>
using namespace std;

int getMinPos(int arr[], int len){
    int minpos = 0;
    for(int i=1;i<len;i++){
        if (arr[i] < arr[minpos])
            minpos = i;
    }
    return minpos;
}

void swap(int &a, int &b){
    int tmp = a;
    a = b;
    b = tmp;
}

void selsort(int arr[], int len){
    for(int i=0;i<len;i++){
        int min = i + getMinPos(&arr[i], len-i);
        swap(arr[i], arr[min]);
    }
}

int main(){
    int a,b,c,d,e,f,g,h,i,j;
    cout<<"enter the lenght a:";
    cin>> a;
    cout<<"enter the lenght b:";
    cin>>b;
    cout<<"enter the lenght c:";
    cin>>c;
    cout<<"enter the lenght d:";
    cin>> d;
    cout<<"enter the lenght e:";
    cin>> e;
    cout<<"enter the lenght f:";
    cin>> f;
    cout<<"enter the lenght g:";
    cin>> g;
    cout<<"enter the lenght h:";
    cin>> h;
    cout<<"enter the lenght i:";
    cin>> i;
    cout<<"enter the lenght j:";
    cin>> j;
    int arr[] = {a, b, c, d, e, f, g, h, i, j};
    
    selsort(arr, 10);
    for(int i=0;i<10;i++)
        printf("%d ", arr[i]);
    printf("\n");
}
